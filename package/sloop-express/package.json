{
  "name": "express-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec tsx -r dotenv/config -r tsconfig-paths/register src/index.ts",
    "start-notwork-because-iam-noob-with-js-ecosystem": "TS_NODE_BASEURL=./dist node  -r dotenv/config  -r tsconfig-paths/register dist/src/index.js",
    "start": "tsx -r dotenv/config -r tsconfig-paths/register src/index.ts",
    "script": "tsx -r dotenv/config",
    "build": "tsc -b --verbose",
    "make-dotenv": "ln -s node_modules/sloop-common/.env .env",
    "db:start": "docker compose -f pg/docker-compose.yaml up -d && echo export DATABASE_URL=postgresql://sloop-test-user:sloop-test-pwd@localhost:5555/sloop-test-user",
    "db:ui": "DATABASE_URL=postgresql://sloop-test-user:sloop-test-pwd@localhost:5555/sloop-test-db prisma studio",
    "db:watch-migrate": "nodemon --watch schema.prisma --exec prisma migrate dev --name idontcare",
    "db:watch-generate": "nodemon --watch schema.prisma --exec prisma generate",
    "db:reset-migrate-mock": "pnpm prisma migrate reset && pnpm prisma migrate dev && pnpm script script/mock-data/generate-mockdata.ts",
    "db:rmm": "pnpm db:reset-migrate-mock",
    "generate-git-info": "node scripts/git_info.js"
  },
  "prisma": {
    "schema": "pg/schema.prisma"
  },
